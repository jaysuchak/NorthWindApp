<core:View xmlns:core="sap.ui.core" controllerName="northwind.northwindapp.supplierDetail" xmlns="sap.m">
	<Page id="idSupplierDetail" title="supplier page" >
		<footer>
			<Bar>
				<contentRight>
					<Button id="Products" text="Products" press="handleProductsPress" />
					<Button id="edit" text="Edit" press="handleEditPress" />
					<Button id="save" text="Save" type="Emphasized" visible="false"
						press="handleSavePress" />
					<Button id="cancel" text="Cancel" visible="false"
						press="handleCancelPress" />
				</contentRight>
			</Bar>
		</footer>
	</Page>
</core:View>